// This is your Prisma schema file

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Newsletter {
  id          String   @id
  name        String
  subject     String
  content     String   @db.Text
  createdAt   DateTime
  updatedAt   DateTime @updatedAt
  publishedAt DateTime?
}

model Cache {
  key       String   @id
  value     String   @db.Text
  expiresAt DateTime
}
